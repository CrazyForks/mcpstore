# MCPStore Vue Frontend - 环境变量配置

# 应用基础配置
VITE_APP_TITLE=MCPStore 管理面板
VITE_APP_VERSION=0.5.0
VITE_APP_DESCRIPTION=强大的MCP服务管理平台

# API配置
VITE_API_BASE_URL=http://localhost:18200
VITE_API_TIMEOUT=30000

# 开发配置
VITE_DEV_PORT=5177
VITE_DEV_HOST=0.0.0.0
VITE_DEV_OPEN=true

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_CONSOLE_LOG=true

# 主题配置
VITE_DEFAULT_THEME=light
VITE_DEFAULT_LANGUAGE=zh-CN

# 性能配置
VITE_ENABLE_GZIP=true
VITE_ENABLE_ANALYZE=false
VITE_DROP_CONSOLE=false
